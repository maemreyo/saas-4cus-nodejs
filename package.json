{
  "name": "modern-backend-template",
  "version": "2.0.0",
  "description": "Production-ready Node.js backend template with TypeScript",
  "main": "dist/app.js",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "rimraf dist && tsc && tsc-alias",
    "start": "node dist/app.js",
    "start:prod": "NODE_ENV=production node dist/app.js",
    "test": "vitest",
    "test:unit": "vitest run --dir src",
    "test:integration": "vitest run --dir tests/integration",
    "test:e2e": "vitest run --dir tests/e2e",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "tsx src/infrastructure/database/seed.ts",
    "db:studio": "prisma studio",
    "docker:build": "docker build -t modern-backend .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docs:generate": "tsx scripts/generate-docs.ts",
    "cli": "tsx src/cli/index.ts"
  },
  "dependencies": {
    "@fastify/autoload": "^5.8.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/multipart": "^8.2.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/sensible": "^5.5.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@prisma/client": "^5.11.0",
    "@sentry/node": "^7.108.0",
    "@sentry/profiling-node": "^7.108.0",
    "argon2": "^0.31.2",
    "bullmq": "^5.4.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "fastify-plugin": "^4.5.1",
    "google-auth-library": "^9.7.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.6",
    "nodemailer": "^6.9.12",
    "otplib": "^12.0.1",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "qrcode": "^1.5.3",
    "rate-limiter-flexible": "^5.0.0",
    "reflect-metadata": "^0.2.1",
    "typedi": "^0.10.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@swc/core": "^1.4.8",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.28",
    "@types/nodemailer": "^6.4.14",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "pino-colada": "^2.2.2",
    "prettier": "^3.2.5",
    "prisma": "^5.11.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vitest": "^1.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/modern-backend-template.git"
  },
  "keywords": [
    "nodejs",
    "backend",
    "template",
    "typescript",
    "fastify",
    "prisma",
    "redis",
    "production-ready"
  ],
  "author": "Your Name",
  "license": "MIT"
}
